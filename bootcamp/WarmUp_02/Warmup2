#include <stdio.h>
#include <string.h>

int main(){
  int num1; 
  int num2;
  printf("number 1: \n");
  scanf("%d", &num1);
//should this be %lf for double?????
  printf("number 2: \n");
  scanf("%d", &num2);
  printf("Add:", (num1 + num2));
  printf("Subtract:",  (num1 - num2));
  printf("Multiply:", (num1 * num2));
  printf("Divide:", (num1 / num2));
  return 0;
}
