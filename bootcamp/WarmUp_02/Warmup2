#include <stdio.h>
#include <string.h>

int main(){
  int num1; 
  int num2;
  int add;
  int sub;
  int mul;
  int div;
  add = num1 + num2;
  sub = num1 - num2;
  mul = num1 * num2;
  div = num1 / num2;
  printf("number 1: \n");
  scanf("%d", &num1);
//should this be %lf for double?????
  printf("number 2: \n");
  scanf("%d", &num2);
  printf("%d", add);
  printf("%d", sub);
  printf("%d",mul);
  printf("%d", div);
  return 0;
}
